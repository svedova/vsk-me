{
  "name": "vsk-me",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "preview": "vite preview",
    "generate": "rm -rf ./dist && vite build --ssrManifest --outDir dist/static && npm run build:server && node ./scripts/prerender",
    "server": "nodemon --watch './**/*.ts' --exec 'node --loader ts-node/esm' src/vite-server.ts"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.13",
    "classnames": "^2.3.2",
    "markdown-it-prism": "^2.3.0",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/glob": "^8.0.0",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.11.8",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "express": "^4.18.2",
    "glob": "^8.0.3",
    "markdown-it": "^13.0.1",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.18",
    "react-router": "^6.4.3",
    "react-router-dom": "^6.4.3",
    "tailwindcss": "^3.2.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.0"
  }
}
